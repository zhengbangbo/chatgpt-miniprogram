page {
    background-color: #fff;
}

page .round-image {
    border-radius: 12rpx;
}

.side-bar-wrapper {
    display: flex;
    height: 100vh;
    margin-bottom: 300rpx;
}

.side-bar-wrapper .content {
    flex: 1;
    margin-bottom: 110rpx;
}

.side-bar-wrapper .section {
    padding: 32rpx 0;
}

.side-bar-wrapper .title {
    padding-left: 40rpx;
    margin-bottom: 8rpx;
    line-height: 52rpx;
}